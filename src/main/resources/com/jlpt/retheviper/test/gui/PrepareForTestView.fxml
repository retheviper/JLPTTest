<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="206.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.jlpt.retheviper.test.controller.PrepareForTestViewControl">
    <children>
        <GridPane layoutX="17.0" layoutY="52.0" prefHeight="99.0" prefWidth="325.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="시험 등급"/>
                <Label text="시험 과목" GridPane.rowIndex="1"/>
                <ComboBox disable="true" prefWidth="150.0" promptText="N2" GridPane.columnIndex="1"/>
                <ComboBox fx:id="setSubject" prefWidth="150.0" promptText="선택" GridPane.columnIndex="1"
                          GridPane.rowIndex="1"/>
                <Label text="시험 시간" GridPane.rowIndex="2"/>
                <ComboBox fx:id="setTimer" prefWidth="150.0" promptText="선택" GridPane.columnIndex="1"
                          GridPane.rowIndex="2"/>
            </children>
        </GridPane>
        <Label layoutX="17.0" layoutY="14.0" text="시험 설정"/>
        <Separator layoutX="17.0" layoutY="40.0" prefHeight="3.0" prefWidth="312.0"/>
        <Button fx:id="startTestButton" layoutX="17.0" layoutY="167.0" mnemonicParsing="false" prefWidth="150.0"
                text="시험 시작"/>
        <Button fx:id="cancelButton" layoutX="179.0" layoutY="167.0" mnemonicParsing="false" prefWidth="150.0"
                text="취소"/>
    </children>
</AnchorPane>
