<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity"
      maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="102.0" prefWidth="248.0"
      xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="com.jlpt.retheviper.test.controller.RegisterStudentViewControl">
    <children>
        <GridPane layoutX="8.0" layoutY="5.0" prefHeight="88.0"
                  prefWidth="231.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER"
                                   hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints halignment="CENTER"
                                   hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TextField fx:id="idField" GridPane.columnIndex="1"/>
                <PasswordField fx:id="passwordField" prefHeight="25.0"
                               prefWidth="80.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="ID"/>
                <Label text="Password" GridPane.rowIndex="1"/>
                <Button fx:id="registerButton" mnemonicParsing="false"
                        prefHeight="25.0" prefWidth="100.0" text="등록" GridPane.rowIndex="2"/>
                <Button fx:id="cancelButton" mnemonicParsing="false"
                        prefHeight="25.0" prefWidth="100.0" text="취소"
                        GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </children>
</Pane>
